{
  "expo": {
    "name": "Tracker NGPRO",
    "slug": "tracker-ngpro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icono.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/icono.png",
      "name": "Manual Tracker NGPRO",
      "shortName": "Manual Tracker NGPRO",
      "description": "Manual de usuario de Tracker NGPRO",
      "lang": "es-ES"
    },
    "plugins": ["expo-router", "expo-font", "expo-web-browser"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
